{
  "name": "pug/yii2",
  "description": "Yii2 Pug (ex Jade) extension",
  "keywords": [
    "yii",
    "pug",
    "jade",
    "view",
    "renderer"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/pug-php/pug-yii2/issues",
    "source": "https://github.com/pug-php/pug-yii2"
  },
  "authors": [
    {
      "name": "Revin Roman",
      "email": "roman@rmrevin.com",
      "homepage": "https://rmrevin.com/"
    },
    {
      "name": "kylekatarnls",
      "email": "pug@selfbuild.fr",
      "homepage": "https://www.phug-lang.com/"
    }
  ],
  "replace": {
    "rmrevin/yii2-pug": "0.0.1",
    "jacmoe/yii2-pug": "1.5.5"
  },
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "^2.0.13",
    "pug-php/pug": "^2.7.6 || ^3.4.1"
  },
  "require-dev": {
    "yidas/yii2-composer-bower-skip": "^2.0",
    "yiisoft/yii2-bootstrap": "^2.0",
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^8.5.15"
  },
  "autoload": {
    "psr-4": {
      "Pug\\Yii\\": ""
    }
  }
}
